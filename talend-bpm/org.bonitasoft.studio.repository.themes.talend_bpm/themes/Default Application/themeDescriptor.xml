<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<theme author="Talend BPM" creationDate="1323704025528" description="The Default application look'n'feel for Talend BPM" imagePreview="preview.png" name="Default Application" provided="true" type="application" updateDate="1323705066960">
  <bindings>
    <binding description="The bar at the top" name="top bar">
      <css-rule>#blueTopLine</css-rule>
      <css-file>application/application/css/process.css</css-file>
    </binding>
    <binding description="default style of buttons" name="Buttons">
      <css-rule>.bonita_form_button</css-rule>
      <css-file>application/application/css/page.css</css-file>
    </binding>
    <binding description="Icone for the link of the user xp" name="User xp link icon">
